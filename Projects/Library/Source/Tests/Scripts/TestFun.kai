
a = 10
msg = "Hello, world"

fun hello()
    print(msg)

fun mul(a, b)
	return a*b

fun fact(n)
    if n == 0 || n == 1
		return 1
	return mul(n, fact(n - 1))

fun outer()
	fun inner(a)
		return a + 2
	s = [1,2,3]
	t = []
	foreach e in s
		t = t + inner(s)
	return t

fun useGen()
	fun gen(n)
		while n > 0
			yield --n
	r = []
	foreach g in gen(3)
		r = r + g
	assert r == [2,1,0]
	
	